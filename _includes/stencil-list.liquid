<section class="stencil-list-section">
  <hr>
  <h2>{{ title }}</h2>
  <ul class="stencil-link-list">
    {%- for post in posts -%}
      <li>
        <a href="{{ post.url }}">{{ post.data.title }}</a>&nbsp;
        {%- if post.data.stencils.size > 0 -%}({{ post.data.stencils.size }}){%- endif -%}
      </li>
    {%- endfor -%}
  </ul>
</section>
